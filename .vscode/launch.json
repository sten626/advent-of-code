      "args": ["${fileBasename}", "input.txt"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${fileDirname}",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "ts-node (sample input)",
      "type": "node",
      "request": "launch",
      "args": ["${fileBasename}", "sample.txt"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${fileDirname}",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
